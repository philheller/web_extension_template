    "@types/node": "^18.11.9",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "cssnano": "^5.1.14",
    "del": "^7.0.0",
    "fancy-log": "^2.0.0",
    "gulp": "^4.0.2",
    "gulp-esbuild": "^0.10.6",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
