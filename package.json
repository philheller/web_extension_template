{
  "name": "web_extension_environment_vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "vite_dev": "vite",
    "vite_preview": "vite preview",
    "gulp": "gulp dev",
    "gulp_build": "gulp build --includeDependencies",
    "dev": "concurrently --kill-others \"gulp dev\" \"tsc && vite build && vite\"",
    "build": "concurrently --kill-others \"tsc && vite build\" \"gulp build --includeDependencies\"",
    "package": "tsc && vite build && gulp package",
    "test": "jest --config jest.config.js"
  },
